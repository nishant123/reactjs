{
  "name": "CIC2.0",
  "version": "1.2.4",
  "description": "Consumer Insights Costing Tool",
  "author": "Dominic Yang",
  "private": true,
  "scripts": {
    "sass:compile": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:*",
    "build:js": "env-cmd -f .env.prod react-scripts build",
    "watch:css": "chokidar \"src/assets/scss/**/*\" -c \"npm run build:css\"",
    "watch:js": "env-cmd -f .env.dev react-scripts start",
    "start": "npm-run-all -p build:css watch:*",
    "build": "npm-run-all -p build:*",
    "build-dev": "npm-run-all -p build-dev:*",
    "build-dev:css": "npm-run-all sass:*",
    "build-dev:js": "env-cmd -f .env.dev react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "> 0.25%",
    "ie >= 11",
    "not op_mini all",
    "not dead"
  ],
  "dependencies": {
    "@alexcambose/react-infinite-scroll": "^1.0.1",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-regular-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.7",
    "@rjsf/bootstrap-4": "^2.4.0",
    "@rjsf/core": "^2.4.0",
    "alasql": "^0.6.5",
    "apexcharts": "^3.26.0",
    "availity-reactstrap-validation": "2.6.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.4.1",
    "chart.js": "2.9.3",
    "chokidar-cli": "2.1.0",
    "classnames": "2.2.6",
    "env-cmd": "^10.1.0",
    "fullcalendar": "3.10.1",
    "google-map-react": "1.1.5",
    "immutability-helper": "^3.1.1",
    "jquery": "^3.5.1",
    "jvectormap-next": "3.1.0",
    "lodash": "^4.17.20",
    "moment": "2.24.0",
